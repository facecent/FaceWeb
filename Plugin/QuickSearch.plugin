[Script]

# Safari内输入快捷键+空格+搜索关键词快速指定搜索引擎搜索
# 先进入设置更改Safari默认搜索为DuckDuckGo

# dlc = DeepL译至中
# dle = DeepL译至英
# dlj = DeepL译至日
# bd = 百度搜索
# bdimg = 百度图片搜索
# ghimg = 谷歌图片搜索
# by = Bing搜索
# wk = 维基搜索
# qm = 七麦搜索
# gh = GitHub搜索
# yh = 油猴搜索
# ggc = Google
# gge = Google
# ggj = Google
# 无快捷键则默认使用Google搜索

# http-request ^https:\/\/duckduckgo\.com\/\?q=dlc\+([^&]+).+ requires-body=1,script-path=https://raw.githubusercontent.com/zZPiglet/Task/master/asset/DeepLQ-search.js,tag=DeepLtoChinese

[Rewrite]
^https:\/\/duckduckgo\.com\/\?q=dlc\+([^&]+).+ 302 https://www.deepl.com/zh/translator#en/zh-hans/
^https:\/\/duckduckgo\.com\/\?q=bd\+([^&]+).+ 302 https://www.baidu.com/s?wd=$1
^https:\/\/duckduckgo\.com\/\?q=bdimg\+([^&]+).+ 302 https://image.baidu.com/search/index?tn=baiduimage&word=$1
^https:\/\/duckduckgo\.com\/\?q=ggimg\+([^&]+).+ 302 https://www.google.com/search?&tbm=isch&q=$1
^https:\/\/duckduckgo\.com\/\?q=by\+([^&]+).+ 302 https://www.bing.com/search?q=$1
^https:\/\/duckduckgo\.com\/\?q=wk\+([^&]+).+ 302 https://wikipedia.org/wiki/$1
^https:\/\/duckduckgo\.com\/\?q=qm\+([^&]+).+ 302 https://www.qimai.cn/search/index/country/cn/search/$1
^https:\/\/duckduckgo\.com\/\?q=gh\+([^&]+).+ 302 https://github.com/search?q=$1
^https:\/\/duckduckgo\.com\/\?q=yh\+([^&]+).+ 302 https://greasyfork.org/zh-CN/scripts?q=$1
^https:\/\/duckduckgo\.com\/\?q=ggc\+([^&]+).+ 302 https://translate.google.com/#view=home&op=translate&sl=auto&tl=zh-CN&text=$1
^https:\/\/duckduckgo\.com\/\?q=gge\+([^&]+).+ 302 https://translate.google.com/#view=home&op=translate&sl=auto&tl=en&text=$1
^https:\/\/duckduckgo\.com\/\?q=ggj\+([^&]+).+ 302 https://translate.google.com/#view=home&op=translate&sl=auto&tl=ja&text=$1
^https:\/\/duckduckgo\.com\/\?q=([^&]+).+ 302 https://www.google.com/search?q=$1

[MitM]
hostname = duckduckgo.com
